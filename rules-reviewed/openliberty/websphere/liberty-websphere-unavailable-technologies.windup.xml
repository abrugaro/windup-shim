liberty-websphere-unavailable-technologies:
  metadata:
    description: This ruleset identifies usage of WebSphere APIs and technologies
      which are not provided by Open Liberty.
    sourceTechnologies:
    - id: websphere
      versionRange: null
    tags: []
    targetTechnologies:
    - id: openliberty
      versionRange: null
  original-file: https://github.com/windup/windup-rulesets/tree/master/rules-reviewed/openliberty/websphere/liberty-websphere-unavailable-technologies.windup.xml
  rules:
    ActivitySessionRule:
      perform:
        hint:
          category-id: mandatory
          effort: '3'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> The Activity Session service is unavailable
            on Liberty. This rule flags the following items: </p> <ul> <li>References
            to APIs in the <code>com.ibm.websphere.ActivitySession</code> package</li>
            <li>The activity session JNDI name <code>java:comp/websphere/UserActivitySession</code></li>
            <li>Elements that are related to activity sessions in Java Platform, Enterprise
            Edition (Java EE) extension deployment descriptors</li> </ul> </div>'
          tags: []
          title: The Activity Session service is unavailable
      when:
        or:
        - annotation-types: []
          javaclass:
            references: '{ActivitySessionRule_0_packages}'
        - filecontent:
            filename: '{*}.java'
            pattern: '{ActivitySessionRule_1_stringLiterals}'
        - xmlfile:
            in: '{ActivitySessionRule_2_filesFilter}'
            matches: //*[local-name()='bean-cache']/@activation-policy[windup:matches(self::node(),
              '{ActivitySessionRule_2_xpath_attributeValue}')]
        - xmlfile:
            in: '{ActivitySessionRule_3_filesFilter}'
            matches: //*[local-name()='local-transaction']/@boundary[windup:matches(self::node(),
              '{ActivitySessionRule_3_xpath_attributeValue}')]
        - xmlfile:
            in: '{ActivitySessionRule_4_filesFilter}'
            matches: //*[local-name()='beanCache']/@activateAt[windup:matches(self::node(),
              '{ActivitySessionRule_4_xpath_attributeValue}')]
        - xmlfile:
            in: '{ActivitySessionRule_5_filesFilter}'
            matches: //*[local-name()='localTransaction']/@boundary[windup:matches(self::node(),
              '{ActivitySessionRule_5_xpath_attributeValue}')]
        - xmlfile:
            in: '{ActivitySessionRule_6_filesFilter}'
            matches: //*[local-name()='activity-sessions']
        - xmlfile:
            in: '{ActivitySessionRule_7_filesFilter}'
            matches: //*[local-name()='activitySessionWebAppExtension']
        - xmlfile:
            in: '{ActivitySessionRule_8_filesFilter}'
            matches: //*[local-name()='activitySessionEJBJarExtension']
      where:
        ActivitySessionRule_0_packages: com.ibm.websphere.ActivitySession\.[^.]+
        ActivitySessionRule_1_stringLiterals: \"[^\"]*java\:comp/websphere/UserActivitySession[^\"]*\"
        ActivitySessionRule_2_filesFilter: ibm-ejb-jar-ext.xml
        ActivitySessionRule_2_xpath_attributeValue: ACTIVITY_SESSION
        ActivitySessionRule_3_filesFilter: ibm-ejb-jar-ext.xml
        ActivitySessionRule_3_xpath_attributeValue: ACTIVITY_SESSION
        ActivitySessionRule_4_filesFilter: ibm-ejb-jar-ext.xmi
        ActivitySessionRule_4_xpath_attributeValue: ACTIVITY_SESSION
        ActivitySessionRule_5_filesFilter: ibm-ejb-jar-ext.xmi
        ActivitySessionRule_5_xpath_attributeValue: ActivitySession
        ActivitySessionRule_6_filesFilter: (ibm-ejb-jar-ext-pme.xml)|(ibm-web-ext-pme.xml)
        ActivitySessionRule_7_filesFilter: ibm-web-ext-pme.xmi
        ActivitySessionRule_8_filesFilter: ibm-ejb-jar-ext-pme.xmi
    AppProfileRule:
      perform:
        hint:
          category-id: mandatory
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> The following WebSphere Application Profiling
            APIs are not available on Liberty: </p> <ul> <li><code>com.ibm.websphere.appprofile</code></li>
            <li><code>com.ibm.websphere.appprofile.accessintent</code></li> </ul>
            <p>You must modify the application so that it can run on Liberty.</p>
            </div>'
          tags: []
          title: The WebSphere Application Profiling APIs are unavailable
      when:
        annotation-types: []
        javaclass:
          references: com.ibm.websphere.appprofile{*}
      where: {}
    AsyncBeansSchedulerRule:
      perform:
        hint:
          category-id: mandatory
          effort: '5'
          links:
          - href: https://www.ibm.com/docs/en/was-liberty/nd?topic=features-concurrency-utilities-java-ee-10
            title: Concurrency Utilities for Java EE 1.0
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: <span class="SubHeader">WebSphere Asynchronous Beans API</span>
            <div class="Text"> <p> This rule flags APIs from the <code>com.ibm.websphere.asynchbeans</code>
            package. These APIs are not provided on Liberty and are marked as deprecated
            in WebSphere Application Server traditional V9.0. The WebSphere Asynchronous
            Beans API are replaced by JSR 236, Concurrency Utilities for Java EE.
            </p> <p> <b>Note:</b> Concurrency Utilities for Java EE provides no replacement
            for the <code>com.ibm.websphere.asynchbeans.pool</code> APIs. </p> <p>
            For information and examples about how to use Concurrency Utilities for
            Java EE instead of Asynchronous beans in WebSphere traditional, see <a
            href="http://www14.software.ibm.com/webapp/wsbroker/redirect?version=cord&amp;product=was-nd-mp&amp;topic=rasb_migrate_to_eeconcurrency"
            title="Opens a new window" onclick="javascript:helpWindow('http://www14.software.ibm.com/webapp/wsbroker/redirect?version=cord&amp;product=was-nd-mp&amp;topic=rasb_migrate_to_eeconcurrency');
            return false;"> Examples to migrate to EE Concurrency from Asynchronous
            beans and CommonJ</a>. </p> <p> For information about the Concurrency
            Utilities for Java EE feature <code>concurrent-1.0</code> in Liberty,
            see the link below </div>
          tags: []
          title: The WebSphere Asynchronous Beans API was superseded by a newer implementation
      when:
        annotation-types: []
        javaclass:
          references: '{AsyncBeansSchedulerRule_0_packages}'
      where:
        AsyncBeansSchedulerRule_0_packages: (com.ibm.websphere.asynchbeans\.[^.]+|com.ibm.websphere.asynchbeans.pool\.[^.]+)
    BatchFeaturePackRule:
      perform:
        hint:
          category-id: mandatory
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: <div class="Text"> <p>WebSphere Batch API and SPI are unavailable
            on Liberty. This rule flags the following packages:</p> <ul> <li><code>com.ibm.websphere.batch*</code></li>
            <li><code>com.ibm.websphere.ci</code></li> <li><code>com.ibm.websphere.grid.spi</code></li>
            <li><code>com.ibm.websphere.longrun</code></li> <li><code>com.ibm.wsspi.batch*</code></li>
            </ul> <p> For information on Liberty Batch, see <a href="http://www.ibm.com/support/techdocs/atsmastr.nsf/WebIndex/WP102544"
            title="Opens a new window" onclick="javascript:helpWindow('http://www.ibm.com/support/techdocs/atsmastr.nsf/WebIndex/WP102544');
            return false;"> IBM WebSphere Liberty Java Batch</a>. </p> <p> For information
            on transitioning to WebSphere Liberty Batch, see the <a href="http://www.ibm.com/support/techdocs/atsmastr.nsf/WebIndex/WP102638"
            title="Opens a new window" onclick="javascript:helpWindow('http://www.ibm.com/support/techdocs/atsmastr.nsf/WebIndex/WP102638');
            return false;"> WebSphere Liberty Batch Migration</a> documentation. </p>
            </div>
          tags: []
          title: The WebSphere Batch API and SPI are unavailable
      when:
        or:
        - annotation-types: []
          javaclass:
            references: '{BatchFeaturePackRule_0_packages}'
        - xmlfile:
            in: '{BatchFeaturePackRule_1_filesFilter}'
            matches: //*[local-name()='local' or local-name()='local-home']/text()[windup:matches(self::node(),
              '{BatchFeaturePackRule_1_xpath_value}')]
      where:
        BatchFeaturePackRule_0_packages: (com.ibm.websphere.batch*\.[^.]+|com.ibm.wsspi.batch*\.[^.]+|com.ibm.websphere.ci\.[^.]+|com.ibm.websphere.grid.spi\.[^.]+|com.ibm.websphere.longrun\.[^.]+)
        BatchFeaturePackRule_1_filesFilter: ejb-jar.xml
        BatchFeaturePackRule_1_xpath_value: com\.ibm\.websphere\.batch\..*
    DetectPolicySetAndWsSecurity:
      perform:
        hint:
          category-id: mandatory
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <span class="SubHeader">Web Services Policy
            Sets are unavailable in Liberty.</span> <p> This rule detects the following
            generated files when using web services Policy Sets in WebSphere traditional:
            </p> <ul> <li>policySet.xml</li> <li>policyAttachments.xml</li> <li>clientPolicyAttachments.xml</li>
            <li>policy.xml</li> <li>bindings.xml</li> <li>bindingDefinition.xml</li>
            </ul> <p> In Liberty, web services Policy Sets are no longer used. Changes
            are needed to migrate an application to the Web Services Policy implementation
            on Liberty. For more information on using WS-Policy in Liberty see <a
            href="https://www.ibm.com/support/knowledgecenter/en/SSEQTP_liberty/com.ibm.websphere.wlp.doc/ae/twlp_dep_policyattach.html"
            title="Opens a new window" onclick="javascript:helpWindow(''https://www.ibm.com/support/knowledgecenter/en/SSEQTP_liberty/com.ibm.websphere.wlp.doc/ae/twlp_dep_policyattach.html'');
            return false;"> Defining web service policy via policy attachment</a>.
            </p> <p> The web services security implementation for Liberty is different
            than WebSphere traditional. If an application is using the WebSphere traditional
            web services security adhering to the OASIS specification changes may
            not be necessary. Review the <a href="https://www.ibm.com/support/knowledgecenter/SS7K4U_liberty/com.ibm.websphere.wlp.zseries.doc/ae/cwlp_wssec_cxf_diff.html"
            title="Opens a new window" onclick="javascript:helpWindow(''https://www.ibm.com/support/knowledgecenter/SS7K4U_liberty/com.ibm.websphere.wlp.zseries.doc/ae/cwlp_wssec_cxf_diff.html'');
            return false;"> WS-Security behavior differences between traditional and
            Liberty</a> for more information about behavior differences that may require
            changes in your application. </p> <p> For more information on the OASIS
            specification see <a href="https://www.ibm.com/support/knowledgecenter/en/SSEQTP_liberty/com.ibm.websphere.wlp.doc/ae/cwlp_wssec_specs.html"
            title="Opens a new window" onclick="https://www.ibm.com/support/knowledgecenter/en/SSEQTP_liberty/com.ibm.websphere.wlp.doc/ae/cwlp_wssec_specs.html'');
            return false;"> Web Services Security specifications and standards</a>.
            </p> </div>'
          tags: []
          title: Web Services Policy Sets are unavailable in Liberty
      when:
        file:
          filename: '{DetectPolicySetAndWsSecurity_0_filenames}'
      where:
        DetectPolicySetAndWsSecurity_0_filenames: policySet\.xml|policyAttachments\.xml|clientPolicyAttachments\.xml|policy\.xml|bindings\.xml|bindingDefinition\.xml
    DetectWSATXML:
      perform:
        hint:
          category-id: optional
          effort: '0'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> Web Services Atomic Transaction (WS-AT)
            Version 1.2 is now available on Liberty. This rule flags the following
            unsupported WS-AT attributes in IBM deployment descriptor extensions:
            </p> <ul> <li><code>execute-using-wsat="true"</code></li> <li><code>sendWSAT="true"</code></li>
            <li><code>send-wsat-context="true"</code></li> <li><code>supportsWSAT="true"</code></li>
            </ul> <p> Liberty does not provide support for WS-AT with EJB global transactions,
            but Liberty applications can use the WS-AT Version 1.2 specification.
            </p> <p> For more information, see the knowledge center topics starting
            at <a href="http://www14.software.ibm.com/webapp/wsbroker/redirect?version=cord&amp;product=was-nd-mp&amp;topic=twlp_dep_wsat_lib"
            title="Opens a new window" onclick="javascript:helpWindow(''http://www14.software.ibm.com/webapp/wsbroker/redirect?version=cord&amp;product=was-nd-mp&amp;topic=twlp_dep_wsat_lib'');
            return false;"> Web Services Atomic Transaction in Liberty</a>. </p> </div>'
          tags: []
          title: Web Services Atomic Transaction (WS-AT) Version 1.2 is available
      when:
        or:
        - xmlfile:
            in: '{DetectWSATXML_0_filesFilter}'
            matches: //*[local-name()='globalTransaction']/@sendWSAT[windup:matches(self::node(),
              '{DetectWSATXML_0_xpath_attributeValue}')]
        - xmlfile:
            in: '{DetectWSATXML_1_filesFilter}'
            matches: //*[local-name()='webGlobalTransaction']/@supportsWSAT[windup:matches(self::node(),
              '{DetectWSATXML_1_xpath_attributeValue}')]
        - xmlfile:
            in: '{DetectWSATXML_2_filesFilter}'
            matches: //*[local-name()='global-transaction']/@send-wsat-context[windup:matches(self::node(),
              '{DetectWSATXML_2_xpath_attributeValue}')]
        - xmlfile:
            in: '{DetectWSATXML_3_filesFilter}'
            matches: //*[local-name()='web-global-transaction']/@execute-using-wsat[windup:matches(self::node(),
              '{DetectWSATXML_3_xpath_attributeValue}')]
      where:
        DetectWSATXML_0_filesFilter: (ibm-web-ext.xmi)|(ibm-ejb-jar-ext.xmi)
        DetectWSATXML_0_xpath_attributeValue: (?i:true)
        DetectWSATXML_1_filesFilter: ibm-web-ext.xmi
        DetectWSATXML_1_xpath_attributeValue: (?i:true)
        DetectWSATXML_2_filesFilter: (ibm-web-ext.xml)|(ibm-eb-jar-ext.xml)
        DetectWSATXML_2_xpath_attributeValue: (?i:true)
        DetectWSATXML_3_filesFilter: ibm-web-ext.xml
        DetectWSATXML_3_xpath_attributeValue: (?i:true)
    DetectWSBAJava:
      perform:
        hint:
          category-id: mandatory
          effort: '5'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: <div class="Text"> <p> Web Services Business Activity (WS-BA) is
            unavailable on Liberty. This rule flags references to APIs in the com.ibm.websphere.wsba
            package and the business activity JNDI name <code>java:comp/websphere/UserBusinessActivity</code>.
            </p> </div>
          tags: []
          title: Web Services Business Activity (WS-BA) is unavailable
      when:
        or:
        - annotation-types: []
          javaclass:
            references: com.ibm.websphere.wsba{*}
        - filecontent:
            filename: '{*}.java'
            pattern: '{DetectWSBAJava_1_stringLiterals}'
      where:
        DetectWSBAJava_1_stringLiterals: \"[^\"]*java\:comp/websphere/UserBusinessActivity[^\"]*\"
    DynamicCacheRule:
      perform:
        hint:
          category-id: optional
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/en/was-liberty/nd?topic=features-distributed-map-interface-dynamic-caching-10
            title: Distributed Map interface for Dynamic Caching
          - href: https://www.ibm.com/docs/en/was-liberty/nd?topic=features-web-response-cache-10
            title: 'Web Response Cache feature documentation '
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> On Liberty, the dynamic cache service is
            provided by the Distributed Map Interface for Dynamic Caching feature,
            <code>distributedMap-1.0</code>, and the Web Response Cache feature, <code>webCache-1.0</code>.
            The implementation has the following restrictions: </p> <ul> <li>No cache
            replication</li> <li>Only the high-performance disk caching mode is supported
            with random and size eviction techniques</li> <li>There is no support
            for Web Services client and server side caching as well as portlet caching
            in the cachespec.xml file</li> <li>Servlet caching of SingleThreadModel
            servlets is not supported</li> <li>Defining cache configuration by using
            properties files is not supported for JAR files that contain only Enterprise
            JavaBeans</li> <li>Limiting the memory cache sizes in megabytes works
            only for 32-bit Java Virtual Machines</li> </ul> <p> This rule flags Java
            code that has references to the following packages: </p> <ul> <li><code>com.ibm.websphere.cache</code></li>
            <li><code>com.ibm.ws.cache</code></li> <li><code>com.ibm.wsspi.cache</code></li>
            <li><code>com.ibm.websphere.servlet.cache</code></li> </ul> <p> The following
            APIs and SPIs are not available in Liberty: </p> <ul> <li><code>com.ibm.websphere.cache.ServletCache</code></li>
            <li><code>com.ibm.wsspi.cache.CacheInstanceResourceBinder</code></li>
            <li><code>com.ibm.wsspi.cache.CacheMonitor</code></li> <li><code>com.ibm.wsspi.cache.ESIStats</code></li>
            <li><code>com.ibm.wsspi.cache.ESIStats$ESIServerStats</code></li> <li><code>com.ibm.wsspi.cache.ESIStats$ESIServerStats$ESIProcessorStats</code></li>
            <li><code>com.ibm.wsspi.cache.ESIStats$ESIServerStats$ESIProcessorStats$ESICacheEntryStats</code></li>
            </ul> <p> For detailed information about dynamic cache on Liberty, see
            the following resources: </p> <ul> <li> <a href="http://www14.software.ibm.com/webapp/wsbroker/redirect?version=phil&amp;product=was-nd-mp&amp;topic=rwlp_restrict#rwlp_restrict__dynacache-restrict"
            title="Opens a new window" onclick="javascript:helpWindow(''http://www14.software.ibm.com/webapp/wsbroker/redirect?version=phil&amp;product=was-nd-mp&amp;topic=rwlp_restrict#rwlp_restrict__dynacache-restrict'');
            return false;"> Liberty:Runtime environment known issues and restrictions</a>
            </li> <li> <a href="https://mediacenter.ibm.com/media/Dynamic+cache+in+Liberty/0_bnh3uidz"
            title="Opens a new window" onclick="javascript:helpWindow(''https://mediacenter.ibm.com/media/Dynamic+cache+in+Liberty/0_bnh3uidz'');
            return false;"> Dynamic cache in Liberty</a> document </li> </div>'
          tags: []
          title: Review use of the dynamic cache service
      when:
        annotation-types: []
        javaclass:
          references: '{DynamicCacheRule_0_packages}'
      where:
        DynamicCacheRule_0_packages: (com.ibm.websphere.cache*\.[^.]+|com.ibm.websphere.servlet.cache*\.[^.]+|com.ibm.ws.cache*\.[^.]+|com.ibm.wsspi.cache*\.[^.]+)
    DynamicQueryRule:
      perform:
        hint:
          category-id: mandatory
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: <div class="Text"> <p> The WebSphere Enterprise JavaBeans (EJB)
            query service is not available on Liberty. </p> <p> This rule flags Java
            code that has references to the <code>com.ibm.websphere.ejbquery</code>
            package. </p> <p> WebSphere EJB query service is used to query entity
            beans, which are not available on Liberty. </p> </div>
          tags: []
          title: The WebSphere EJB Query API is unavailable
      when:
        or:
        - annotation-types: []
          javaclass:
            references: '{DynamicQueryRule_0_packages}'
        - xmlfile:
            in: '{DynamicQueryRule_1_filesFilter}'
            matches: //*[local-name()='home' or local-name()='remote']/text()[windup:matches(self::node(),
              '{DynamicQueryRule_1_xpath_value}')]
      where:
        DynamicQueryRule_0_packages: com.ibm.websphere.ejbquery\.[^.]+
        DynamicQueryRule_1_filesFilter: ejb-jar.xml
        DynamicQueryRule_1_xpath_value: com\.ibm\.websphere\.ejbquery\..*
    I18nRule:
      perform:
        hint:
          category-id: mandatory
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> The following WebSphere i18n APIs and SPIs
            are not available on Liberty: </p> <ul> <li><code>com.ibm.websphere.i18n.context</code></li>
            <li><code>com.ibm.wsspi.i18n.context.util</code></li> </ul> <p>You must
            modify the application so that it can run on Liberty.</p> </div>'
          tags: []
          title: The WebSphere i18n APIs and SPIs are unavailable
      when:
        annotation-types: []
        javaclass:
          references: '{I18nRule_0_packages}'
      where:
        I18nRule_0_packages: (com.ibm.websphere.i18n.context\.[^.]+|com.ibm.wsspi.i18n.context.util\.[^.]+)
    MTOMRule:
      perform:
        hint:
          category-id: optional
          effort: '1'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> On Liberty, each DataHandler object can
            be written to an output stream only once. Writing a DataHandler object
            to an OutputStream object more than once results in an empty file. After
            you call the <code>javax.activation.DataHandler.writeTo(OutputStream)</code>
            method, you cannot pass the DataHandler object to another method, return
            it, or store it for later use. </p> <p> As a workaround, you can create
            a new DataHandler object and initialize the DataHandler object with the
            content that was already retrieved from the existing DataHandler object
            using the <code>writeTo</code> method. For example: </p> <table class="code">
            <tbody> <tr> <td> <span class="Code"> <span class="JavaType">File</span>
            f = <span class="JavaKeyword">new</span> File("received_image");<br> <span
            class="JavaKeyword">if</span> (f.exists()) {<br> <span class="indent"></span>f.delete();<br>
            }<br> <br> <span class="JavaType">FileOutputStream</span> fos = <span
            class="JavaKeyword">new</span> FileOutputStream(f);<br> <br> // Write
            the DataHandler object to the output stream.<br> img_in.writeTo(fos);<br>
            <br> // Create a new DataHandler object and initialize it with<br> //
            the content that was retrieved using the writeTo method above.<br> <br>
            <span class="JavaType">FileDataSource</span> fos_out = <span class="JavaKeyword">new</span>
            FileDataSource(f);<br> <br> <span class="JavaType">DataHandler</span>
            img_out = <span class="JavaKeyword">new</span> DataHandler(fos_out);<br>
            <br> <br> <span class="JavaKeyword">return</span> img_out; </span> </td>
            </tr> </tbody> </table> </div>'
          tags: []
          title: Review use of the javax.activation.DataHandler object
      when:
        annotation-types: []
        javaclass:
          references: javax.activation.DataHandler.writeTo(java.io.OutputStream)
        location: METHOD_CALL
      where: {}
    SOAPOverJMSRule:
      perform:
        hint:
          category-id: mandatory
          effort: '5'
          links:
          - href: https://www.ibm.com/docs/wamt?topic=rules-open-liberty-migration-in-windup
            title: Open Liberty migration rules in Windup
          message: '<div class="Text"> <p> Liberty does not support Simple Object
            Access Protocol (SOAP) over Java Message Service (JMS). References to
            the following namespaces indicate usage of SOAP over JMS: </p> <ul> <li>http://www.w3.org/2010/soapjms/</li>
            <li>http://www.w3.org/2010/soapjms/soap1.1</li> <li>http://www.w3.org/2010/soapjms/soap1.2</li>
            </ul> This rule flags the following items: <p></p> <ul> <li><code>javax.xml.ws.BindingType</code>
            annotations with a <code>value</code> attribute that references one of
            the described namespaces</li> <li>WSDL files with a <code>transport</code>
            attribute on the <span class="XmlDefinition">&lt;binding&gt;</span> element
            that references one of the described namespaces</li> </ul> </div>'
          tags: []
          title: SOAP over Java Message Service (JMS) is unavailable
