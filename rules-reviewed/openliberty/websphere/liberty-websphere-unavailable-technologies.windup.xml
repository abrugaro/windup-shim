          javaclass:
            references: com.ibm.websphere.management.AdminService.getProcessName({*})
          location: METHOD_CALL
        - annotation-types: []
          javaclass:
            references: com.ibm.ejs.ras.RasHelper.getServerName({*})
          location: METHOD_CALL
        - annotation-types: []
          javaclass:
